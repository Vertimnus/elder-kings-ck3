template preview_portrait_base
{
	using = portrait_editor_standard_head
	datacontext = [PortraitEditorWindow.GetPortraitDataContext]
	portrait_texture = "[GetPortraitTextureFromDna('type=male|environment=environment_standard|camera=camera_head|dna=8gDwAAQGCAUAGAAZALAAsADPAM8A4QDhAKUApQDmAOYD0APQAP4A/gL9Av0AmQCZAHkAAAAAAAAAAAAAAAAAAAAAAAAA/wD/', PdxGetWidgetScreenSize(PdxGuiWidget.Self))]"
	tooltip_enabled = no
	alwaystransparent = no
}
template preview_portrait_200
